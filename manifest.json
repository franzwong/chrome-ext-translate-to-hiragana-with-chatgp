{
    "manifest_version": 3,
    "name": "Translate to Hiragana",
    "version": "1.0",
    "permissions": ["contextMenus"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
  }
  